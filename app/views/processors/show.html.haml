%p#notice= notice

%h1=@processor.name
%h3=@processor.item

=link_to 'statements', "", :class => 'toggle'

%div.statements.none
	%h3 Statements

	-@processor_rdf.each do |solution|
		%p=solution.inspect

-if @processor.details
	%div
		%ul.details
			-@processor.details.each_pair do |key, value|
				%li
					%strong=key
					=simple_format(value)

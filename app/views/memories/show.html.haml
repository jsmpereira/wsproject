%p#notice= notice

%h1=@memory.name
%h3=@memory.item

=link_to 'statements', "", :class => 'toggle'

%div.statements.none
	%h3 Statements

	-@memory_rdf.each do |solution|
		%p=solution.inspect

-if @memory.details
	%div
		%ul.details
			-@memory.details.each_pair do |key, value|
				%li
					%strong=key
					=simple_format(value)